{
  "server": false,
  "ui": true,
  "data_dir": "/opt/consul/data",
  "client_addr": "0.0.0.0",
  "encrypt": "{{ encrypt_key }}",
  "ca_file": "{{ consul_ca_file }}",
  "cert_file": "{{ consul_cert_file }}",
  "key_file": "{{ consul_key_file }}",
  "verify_incoming": true,
  "verify_outgoing": true,
  "advertise_addr": "{{ ansible_eth0.ipv4.address }}",

  "retry_join_ec2": {
    "tag_key": "{{ ec2_tag_key }}",
    "tag_value": "{{ ec2_tag_value }}"
  }
}
